<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="SMCRepositoryID"
            class="Structure.Navigator"
            id="ResourceListID"
            name="ResourceListNAME"
            restorable="true">
      </view>
      <category
            id="SMCRepositoryID"
            name="SMCRepositoryName">
      </category>
   </extension>
   
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="esempionewtab.editors.MultiPageEditor"
            contributorClass="esempionewtab.editors.MultiPageEditorContributor"
            extensions="asts, anctl, ont"
            icon="icons/sample.gif"
            id="esempionewtab.editors.MultiPageEditor"
            name="Sample Multi-page Editor">
      </editor>
   </extension>

</plugin>
